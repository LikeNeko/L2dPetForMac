{
  "private": true,
  "scripts": {
    "start": "find ./dist -type f -name \"*hot*\"| xargs rm | webpack-dev-server --progress",
    "build": "webpack -d --hide-modules",
    "build:prod": "webpack -p --hide-modules",
    "test": "tsc --noEmit",
    "watch": "tsc -w",
    "lint": "eslint src -f codeframe --ext .ts",
    "lint:fix": "eslint src -f codeframe --ext .ts --fix",
    "serve": "serve . -p 5000",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/matter-js": "^0.17.7",
    "serve": "^11.3.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "matter-js": "^0.18.0",
    "tern": "^0.24.3",
    "twgl.js": "^4.22.0",
    "whatwg-fetch": "^3.0.0"
  }
}
